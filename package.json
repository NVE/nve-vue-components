{
  "name": "@norges-vassdrags-og-energidirektorat/nve-vue-components",
  "version": "0.0.1",
  "private": false,
  "description": "NVE's standard komponentbibliotek er implementert som web-components. Mer info om dette finner du her: https://designsystem.nve.no/ . Dette prosjektet inneholder komponenter som bygger på NVE sitt designsystem, men som blir ansett som for komplekse til å være rene web-komponenter.",
  "author": "IUR NVE",
  "license": "MIT",
  "main": "./dist/nve-vue-components.umd.cjs",
  "module": "./dist/nve-vue-components.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nve-vue-components.js",
      "require": "./dist/nve-vue-components.umd.cjs"
    }
  },
  "files": [
    "dist",
    "src/components/"
  ],
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --ext .vue,.js,.ts --fix",
    "demo": "cd demo && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NVE/nve-vue-components.git"
  },
  "keywords": ["nve", "vue3", "components", "designsystem"],
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@vueuse/core": "^13.0.0",
    "nve-designsystem": "^1.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "typescript": "~5.8.3",
    "vue-tsc": "^2.2.8",
    "vite": "^6.2.5",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "@types/node": "^22.14.0"
  }
}
