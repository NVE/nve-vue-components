{
  "name": "@norges-vassdrags-og-energidirektorat/nve-vue-components",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.3.0",
  "private": false,
  "description": "NVE's standard komponentbibliotek er implementert som web-components. Mer info om dette finner du her: https://designsystem.nve.no/ . Dette prosjektet inneholder komponenter som bygger på NVE sitt designsystem, men som blir ansett som for komplekse til å være rene web-komponenter.",
  "author": "IUR NVE",
  "license": "MIT",
  "type": "module",
  "main": "./dist/nve-vue-components.umd.js",
  "module": "./dist/nve-vue-components.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/nve-vue-components.es.js",
      "require": "./dist/nve-vue-components.umd.js"
    }
  },
  "files": [
    "dist",
    "src/components/"
  ],
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --ext .vue,.js,.ts --fix",
    "demo": "cd demo && npm run dev",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NVE/nve-vue-components.git"
  },
  "keywords": [
    "nve",
    "vue3",
    "components",
    "designsystem"
  ],
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vueuse/core": "^13.2.0",
    "eslint-plugin-vue": "^10.1.0",
    "husky": "^9.1.7",
    "nve-designsystem": "^1.6.2",
    "vue-eslint-parser": "^10.1.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/ui": "^3.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-vue": "^10.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.3",
    "typescript": "~5.8.3",
    "vite": "^6.2.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.8"
  }
}
